{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text"
    },
    "private": {
      "type": "boolean"
    },
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "regex": "^(\\/[^\\s]*)|(https?:\\/\\/([a-zA-Z0-9-]+\\.)+[a-zA-Z]{2,}(\\/[^\\s]*)?)$"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "customField",
      "default": "[]",
      "options": [
        "Dashboard",
        "Herobanner",
        "Landing Page",
        "Portfolio",
        "Website"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
